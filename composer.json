{
    "name": "framgia-education/laravel-omniauth-hrsystem",
    "description": "Framgia Education Laravel wrapper around OAuth 2 libraries.",
    "keywords": ["oauth", "laravel", "Framgia"],
    "license": "MIT",
    "authors": [
        {
            "name": "Framgia Vietnam",
            "email": "info@framgia.vn"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/contracts": "~5.4",
        "illuminate/http": "~5.4",
        "illuminate/support": "~5.4"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "Framgia\\Education\\Auth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
